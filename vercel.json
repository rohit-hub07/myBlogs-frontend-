{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://myblogs-backend-91ie.onrender.com/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://myblogs-backend-91ie.onrender.com/api/:path*/"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
